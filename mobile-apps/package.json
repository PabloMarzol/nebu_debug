    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.2.1",
    "detox": "^20.13.5",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
